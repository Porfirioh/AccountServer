<div id="menu1">&nbsp;<button id="home" href="/home/default.html">Home</button><button id="customer" href="/customer/default.html">Customers</button><button id="supplier" href="/supplier/default.html">Suppliers</button><button id="banking" href="/banking/default.html">Banking</button><button id="investments" href="/investments/default.html">Investments</button><button id="accounting" href="/accounting/default.html">Accounting</button><button id="members" href="/members/default.html">Members</button><button id="reports" href="/reports/default.html">Reports</button><button id="admin" href="/admin/default.html">Admin</button></div>
<div id="menu2">&nbsp;{{#each Menu}}<button id="{{Id}}" href="{{Url}}"{{#if Disabled}} disabled="disabled"{{/if}}>{{Text}}</button>{{/each}}</div>
<div id="menu3">&nbsp;</div>
</div>
<div id="spacer"></div>
<div id="body">
<div id="message">{{Message}}&nbsp;</div>
{{Body}}
</div>
